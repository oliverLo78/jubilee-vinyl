{
  "name": "jubilee",
  "version": "1.0.0",
  "description": "Custom vinyl creation app with Spotify integration",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install-all": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build",
    "build-pwa": "cd client && GENERATE_SOURCEMAP=false npm run build",
    "start-client": "cd client && HOST=127.0.0.1 npm start",
    "test": "cd client && npm test",
    "eject": "cd client && npm run eject",
    "seed": "cd server && npm run seed",
    "client": "cd client && npm start",
    "server": "cd server && npm run dev",
     "test-db": "node test-database.js",
    "heroku-postbuild": "npm run install-all && npm run build"
  },
  
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "apollo-server-express": "^3.12.1",
    "graphql": "^16.8.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "axios": "^1.5.0",
    "qs": "^6.11.2"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}

